{
  "name": "www",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "toast incremental",
    "prepare": "breadbox --dest public/web_modules",
    "breadbox": "breadbox --dest public/web_modules",
    "develop": "babel --config-file=./sector-babel-config.js src/pages/ --out-dir public --watch",
    "develop-project-src": "babel --no-babelrc --presets=@babel/preset-react --plugins=@babel/plugin-proposal-class-properties,@snowpack/babel-plugin-package-import src/ --out-dir public"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@mdx-js/preact": "1.5.7",
    "@sector/babel-plugin-package-import": "^0.3.0",
    "fetch-sector-docs": "^1.0.0",
    "parse-numeric-range": "^1.2.0",
    "prismjs": "^1.20.0",
    "react-helmet": "^6.0.0-beta.2",
    "rehype-autolink-headings": "^3.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-prism-mdx": "1.0.0",
    "rehype-slug": "^3.0.0",
    "toast": "0.2.8",
    "unified": "^9.0.0"
  },
  "devDependencies": {
    "@sector/breadbox": "^0.0.4",
    "node-getpocket": "^1.0.0"
  },
  "snowpack": {
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "@emotion/core",
      "@mdx-js/preact",
      "react-helmet",
      "preact/compat"
    ]
  }
}
