{
  "name": "www",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "toast incremental .",
    "prepare": "breadbox --dest public/web_modules",
    "breadbox": "breadbox --dest public/web_modules",
    "develop": "babel --config-file=./sector-babel-config.js src/pages/ --out-dir public --watch",
    "develop-project-src": "babel --no-babelrc --presets=@babel/preset-react --plugins=@babel/plugin-proposal-class-properties,@snowpack/babel-plugin-package-import src/ --out-dir public"
  },
  "type": "module",
  "dependencies": {
    "@babel/preset-react": "7.10.4",
    "@emotion/core": "^20",
    "@mdx-js/preact": "2.0.0-next.8",
    "@sector/babel-plugin-package-import": "^0.3.0",
    "@stripe/stripe-js": "^1.9.0",
    "fetch-sector-docs": "^1.0.0",
    "json5": "^2.1.3",
    "parse-numeric-range": "^1.2.0",
    "patch-package": "^6.2.2",
    "prismjs": "^1.20.0",
    "react-helmet": "^6.1.0",
    "rehype-autolink-headings": "^3.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-prism-mdx": "1.0.0",
    "rehype-slug": "^3.0.0",
    "remark-stringify": "^8.1.1",
    "toastrs": "^0.0.17",
    "unified": "^9.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@sector/breadbox": "^0.0.4",
    "node-getpocket": "^1.0.0"
  },
  "snowpack": {
    "exclude": ["public/**/*", "toast.js", "fetch-mdx-post-files.js"],
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "@emotion/core",
      "@mdx-js/preact",
      "react-helmet",
      "preact/compat",
      "@stripe/stripe-js"
    ]
  }
}
