{
  "name": "christopherbiscardi.github.com",
  "version": "1.0.0",
  "description": "my website",
  "main": "index.js",
  "repository": "git@github.com:ChristopherBiscardi/christopherbiscardi.github.com.git",
  "author": "ChristopherBiscardi <chris@christopherbiscardi.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "update-sens8": "git subtree pull --prefix sens8 git@github.com:ChristopherBiscardi/sens8.git master --squash"
  },
  "private": true,
  "workspaces": ["./sens8/packages/*", "."],
  "lint-staged": {
    "*.{js,jsx}": ["prettier --parser flow --write", "git add"],
    "*.json": ["prettier --parser json --write", "git add"],
    "*.{graphql,gql}": ["prettier --parser graphql --write", "git add"],
    "*.{md,markdown}": ["prettier --parser markdown --write", "git add"],
    "*.{css,scss}": ["prettier --parser css --write", "git add"]
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.5",
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.4",
    "@rehooks/component-size": "^1.0.2",
    "@sens8/component-typography": "*",
    "emotion-theming": "^10.0.4",
    "gatsby": "2.0.81",
    "gatsby-mdx": "ci",
    "gatsby-plugin-emotion": "3.0.1",
    "gatsby-plugin-google-analytics": "2.0.8",
    "gatsby-plugin-offline": "2.0.19",
    "gatsby-plugin-react-helmet": "3.0.4",
    "gatsby-remark-prismjs": "3.1.4",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "palx": "^1.0.3",
    "prettier": "^1.15.3",
    "prismjs": "^1.14.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-emotion": "^10.0.0",
    "react-helmet": "^5.2.0",
    "react-pose": "^4.0.4",
    "react-prism": "^4.3.2",
    "react-sidebar": "^3.0.2",
    "react-use": "^5.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "slugify": "^1.3.4",
    "styled-system": "^3.1.11",
    "superbox": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@mdx-js/loader": "^0.16.6",
    "@mdx-js/mdx": "^0.16.6",
    "gatsby-source-filesystem": "2.0.12",
    "loader-utils": "^1.1.0"
  }
}
